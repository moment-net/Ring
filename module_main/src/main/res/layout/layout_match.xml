<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <com.alan.mvvm.common.views.BottomToTopFinishLayout
        android:id="@+id/cl_finish"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen195"
        android:layout_marginTop="@dimen/dimen60"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/cl_bg"
            android:layout_width="0dp"
            android:layout_height="0dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent">

            <com.airbnb.lottie.LottieAnimationView
                android:id="@+id/lv_match"
                android:layout_width="@dimen/dimen0"
                android:layout_height="@dimen/dimen0"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:lottie_imageAssetsFolder="images"
                app:lottie_fileName="lottie_match.json" />

            <TextView
                android:id="@+id/tv_num"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="16dp"
                android:layout_marginTop="16dp"
                android:text="免费匹配次数：3"
                android:textColor="@color/white"
                android:textSize="12sp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/iv_close"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:layout_marginTop="16dp"
                android:layout_marginEnd="16dp"
                android:padding="@dimen/dimen3"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/icon_match_close" />

            <ImageView
                android:id="@+id/iv_avatar"
                android:layout_width="52dp"
                android:layout_height="52dp"
                android:layout_marginTop="30dp"
                android:src="@drawable/icon_placeholder"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <leifu.shapelibrary.ShapeView
                android:id="@+id/tv_age"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dimen16"
                android:layout_marginTop="10dp"
                android:drawableStart="@drawable/icon_home_girl"
                android:drawablePadding="@dimen/dimen3"
                android:gravity="center_vertical"
                android:includeFontPadding="false"
                android:paddingStart="@dimen/dimen5"
                android:paddingTop="0dp"
                android:paddingEnd="@dimen/dimen5"
                android:paddingBottom="0dp"
                android:text="19岁  准格尔"
                android:textColor="@color/_FF517A"
                android:textSize="10sp"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/iv_avatar"
                app:sCorners="3dp"
                app:sSizeHeight="@dimen/dimen20"
                app:sSolidColor="@color/_FFE6EC"
                app:sStrokeWidth="0dp" />

            <TextView
                android:id="@+id/tv_desc"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="8dp"
                android:layout_marginBottom="18dp"
                android:ellipsize="end"
                android:maxLines="1"
                android:singleLine="true"
                android:text="番茄会害羞正在邀请你边吃边聊"
                android:textColor="@color/white"
                android:textSize="14sp"
                app:layout_constraintBottom_toTopOf="@+id/tv_chat"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toBottomOf="@+id/tv_age" />

            <leifu.shapelibrary.ShapeView
                android:id="@+id/tv_chat"
                android:layout_width="100dp"
                android:layout_height="30dp"
                android:layout_marginBottom="20dp"
                android:gravity="center"
                android:padding="0dp"
                android:text="立即接通"
                android:textColor="@color/_FFD54A"
                android:textSize="14sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_chainStyle="packed"
                app:layout_constraintStart_toStartOf="parent"
                app:sCorners="6dp"
                app:sSolidColor="@color/white"
                app:sStrokeWidth="0dp" />

            <androidx.constraintlayout.widget.Group
                android:id="@+id/group_all"
                android:layout_width="@dimen/dimen0"
                android:layout_height="@dimen/dimen0"
                android:visibility="gone"
                app:constraint_referenced_ids="tv_num,iv_avatar,iv_close,tv_age,tv_desc,tv_chat" />

        </androidx.constraintlayout.widget.ConstraintLayout>

    </com.alan.mvvm.common.views.BottomToTopFinishLayout>


</androidx.constraintlayout.widget.ConstraintLayout>