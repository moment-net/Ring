<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <leifu.shapelibrary.ShapeView
        android:id="@+id/tv_content"
        android:layout_width="match_parent"
        android:layout_height="@dimen/dimen36"
        android:gravity="center"
        android:text="宿舍"
        android:textColor="@color/_803A3A3A"
        android:textSize="16sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:padding="0dp"
        app:sSizeHeight="@dimen/dimen36"
        app:sGravity="center"
        app:sCorners="18dp"
        app:sSolidColor="@color/white"
        app:sStrokeColor="@color/_803A3A3A"
        app:sStrokeWidth="1dp" />


</androidx.constraintlayout.widget.ConstraintLayout>