<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="@dimen/dimen84"
    android:background="@drawable/shape_home_manager10">


    <TextView
        android:id="@+id/main_textview8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:includeFontPadding="false"
        android:text="09:43"
        android:textColor="@color/black"
        android:textSize="@dimen/sp26"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/main_textview10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:text="2月31号"
        android:textColor="@color/black"
        android:textSize="@dimen/sp14"
        app:layout_constraintBaseline_toBaselineOf="@+id/main_textview8"
        app:layout_constraintStart_toEndOf="@+id/main_textview8" />

    <TextView
        android:id="@+id/main_textview9"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="15dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="@dimen/dimen10"
        android:text="今天小李要喝三杯水李要喝三杯水"
        android:textColor="@color/black"
        android:textSize="@dimen/sp14"
        app:layout_constraintEnd_toStartOf="@+id/iv_avatar"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/main_textview8" />


    <ImageView
        android:id="@+id/iv_avatar"
        android:layout_width="@dimen/dimen70"
        android:layout_height="@dimen/dimen70"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/icon_placeholder" />

</androidx.constraintlayout.widget.ConstraintLayout>